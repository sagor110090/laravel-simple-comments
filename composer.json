{
    "name": "sagor110090/laravel-simple-comments",
    "description": "Add comments to your Laravel application",
    "keywords": [
        "Sagor110090",
        "comments",
        "laravel-comments"
    ],
    "homepage": "https://github.com/sagor110090",
    "license": "MIT",
    "authors": [
        {
            "name": "mehedi hasan sagor",
            "email": "mehedihasansagor.cse@gmail.com",
            "homepage": "https://mehedihasansagor.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6|^5.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Sagor110090\\Comments\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sagor110090\\Comments\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sagor110090\\Comments\\CommentsServiceProvider"
            ]
        }
    }
}
